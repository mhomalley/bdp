%h1
  Edit user's name and password for
  = @user.user_name
= error_messages_for :user

- form_for(:user, :url => admin_user_path(@user), :html => { :method => :put }) do |@f|
  %p
    = render(:partial => 'form')
  %p
    = submit_tag "Update"

= link_to 'List All Users', admin_users_path
